#include "main.h"
#include "MOTOR.h"
#include "MotorControl.h"

MotorControl M1(LED_Pin1_GPIO_Port, LED_Pin1_Pin);

void frente(int time) {
	M1.MotorControl(1);


	HAL_Delay(time);
}

void parar(int time) {
	M1.MotorControl(0);
	M2.MotorControl(1);

	HAL_Delay(time);
}

void mainCpp() {
	frente(1000);
	parar(1000);
}

