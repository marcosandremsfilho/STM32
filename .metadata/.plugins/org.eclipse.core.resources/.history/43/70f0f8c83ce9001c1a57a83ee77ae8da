#include "main.h"

void mainCpp() {
	HAL_GPIO_WritePin(LED1_GPIO_Port, LED1_Pin, (GPIO_PinState) 1);
	HAL_GPIO_WritePin(LED2_GPIO_Port, LED2_Pin, (GPIO_PinState) 1);
	HAL_GPIO_WritePin(LED3_GPIO_Port, LED3_Pin, (GPIO_PinState) 1);
	HAL_GPIO_WritePin(LED4_GPIO_Port, LED4_Pin, (GPIO_PinState) 1);
	HAL_Delay(500);
	HAL_GPIO_WritePin(LED1_GPIO_Port, LED1_Pin, (GPIO_PinState) 0);
	HAL_GPIO_WritePin(LED2_GPIO_Port, LED2_Pin, (GPIO_PinState) 0);
	HAL_GPIO_WritePin(LED3_GPIO_Port, LED3_Pin, (GPIO_PinState) 0);
	HAL_GPIO_WritePin(LED4_GPIO_Port, LED4_Pin, (GPIO_PinState) 0);
	HAL_Delay(500);
}

