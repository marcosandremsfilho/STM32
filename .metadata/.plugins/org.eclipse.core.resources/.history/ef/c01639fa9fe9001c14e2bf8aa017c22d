#include "main.h"
#include "MOTOR.h"

MOTOR M1(LED_Pin1_GPIO_Port, LED_Pin1_Pin);
MOTOR M2(LED_Pin2_GPIO_Port, LED_Pin2_Pin);

void frente(int time) {
	M1.MotorControl(1);
	M2.MotorControl(0);

	HAL_Delay(time);
}

void parar(int time) {
	M1.MotorControl(0);
	M2.MotorControl(1);

	HAL_Delay(time);
}



