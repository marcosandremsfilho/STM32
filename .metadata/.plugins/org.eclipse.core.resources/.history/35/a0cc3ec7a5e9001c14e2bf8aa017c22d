#include "main.h"
#include "MotorControl.h"
#include "MOTOR.h"

void MotorControl :: frente(int time, int control) {
	HAL_GPIO_WritePin(_Port, _Pin, (GPIO_PinState) control);
	HAL_Delay(time);
}

void MotorControl :: parar(int time, int control) {
	HAL_GPIO_WritePin(_Port, _Pin, (GPIO_PinState) control);
	HAL_Delay(time);
}

MotorControl::~MotorControl() {
	// TODO Auto-generated destructor stub
}

