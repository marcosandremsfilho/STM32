#include "SensorIR.h"
#include "main.h"

SensorIR::SensorIR(GPIO_TypeDef* Port, uint16_t Pin) {
	// TODO Auto-generated constructor stub
	_Port = Port;
	_Pin = Pin;
}

int SensorIR :: leSensor() {
	int value;

	value = HAL_GPIO_ReadPin(_Port, _Pin);

	return value;
}

