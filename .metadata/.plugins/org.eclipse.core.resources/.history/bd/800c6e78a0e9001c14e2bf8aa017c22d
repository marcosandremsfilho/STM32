#include "main.h"
#include "mainCpp.h"
#include "MOTOR.h"

void frente(int time) {
	M1.MotorControl(1);
	M2.MotorControl(0);

	HAL_Delay(time);
}

void parar(int time) {
	M1.MotorControl(0);
	M2.MotorControl(1);

	HAL_Delay(time);
}

void mainCpp() {
	frente(1000);
	parar(1000);
}

